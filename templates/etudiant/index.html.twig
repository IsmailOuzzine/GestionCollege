{% extends 'base.html.twig' %}

{% block title %}Etudiant index{% endblock %}

{% block body %}
    <h1>Etudiant index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Cne</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Sexe</th>
                <th>Nomclasse</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etudiant in etudiants %}
            <tr>
                <td>{{ etudiant.cne }}</td>
                <td>{{ etudiant.nom }}</td>
                <td>{{ etudiant.prenom }}</td>
                <td>{{ etudiant.sexe }}</td>
                <td>{{ etudiant.nomclasse }}</td>
                <td>
                    <a href="{{ path('etudiant_show', {'cne': etudiant.cne}) }}">show</a>
                    <a href="{{ path('etudiant_edit', {'cne': etudiant.cne}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('etudiant_new') }}">Create new</a>
{% endblock %}
